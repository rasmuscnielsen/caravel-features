{
    "name": "rasmuscnielsen/laravel-features",
    "license": "CC-BY-SA-4.0",
    "autoload": {
        "psr-4": {
            "LaravelFeatures\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.5",
        "illuminate/database": "^5.5",
        "illuminate/events": "^5.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.3",
        "laravel/laravel": "5.6",
        "doctrine/dbal": "^2.5",
        "orchestra/testbench": "~3.5",
        "nunomaduro/collision": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFeatures\\FeaturesServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFeatures\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "style": "./vendor/bin/php-cs-fixer fix ./src --rules=@PSR2,@Symfony,ordered_class_elements,ordered_imports --verbose"
    }
}
